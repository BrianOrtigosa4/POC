#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Usuario/Desktop/UTN/Desarrollo de Software/PoC/node_modules/.pnpm/mustache@2.3.2/node_modules/mustache/bin/node_modules:/mnt/c/Users/Usuario/Desktop/UTN/Desarrollo de Software/PoC/node_modules/.pnpm/mustache@2.3.2/node_modules/mustache/node_modules:/mnt/c/Users/Usuario/Desktop/UTN/Desarrollo de Software/PoC/node_modules/.pnpm/mustache@2.3.2/node_modules:/mnt/c/Users/Usuario/Desktop/UTN/Desarrollo de Software/PoC/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Usuario/Desktop/UTN/Desarrollo de Software/PoC/node_modules/.pnpm/mustache@2.3.2/node_modules/mustache/bin/node_modules:/mnt/c/Users/Usuario/Desktop/UTN/Desarrollo de Software/PoC/node_modules/.pnpm/mustache@2.3.2/node_modules/mustache/node_modules:/mnt/c/Users/Usuario/Desktop/UTN/Desarrollo de Software/PoC/node_modules/.pnpm/mustache@2.3.2/node_modules:/mnt/c/Users/Usuario/Desktop/UTN/Desarrollo de Software/PoC/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mustache@2.3.2/node_modules/mustache/bin/mustache" "$@"
else
  exec node  "$basedir/../../../../../mustache@2.3.2/node_modules/mustache/bin/mustache" "$@"
fi
