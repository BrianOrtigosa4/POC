#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Usuario/Desktop/UTN/Desarrollo de Software/PoC/node_modules/.pnpm/testcafe@3.6.2/node_modules/testcafe/bin/node_modules:/mnt/c/Users/Usuario/Desktop/UTN/Desarrollo de Software/PoC/node_modules/.pnpm/testcafe@3.6.2/node_modules/testcafe/node_modules:/mnt/c/Users/Usuario/Desktop/UTN/Desarrollo de Software/PoC/node_modules/.pnpm/testcafe@3.6.2/node_modules:/mnt/c/Users/Usuario/Desktop/UTN/Desarrollo de Software/PoC/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Usuario/Desktop/UTN/Desarrollo de Software/PoC/node_modules/.pnpm/testcafe@3.6.2/node_modules/testcafe/bin/node_modules:/mnt/c/Users/Usuario/Desktop/UTN/Desarrollo de Software/PoC/node_modules/.pnpm/testcafe@3.6.2/node_modules/testcafe/node_modules:/mnt/c/Users/Usuario/Desktop/UTN/Desarrollo de Software/PoC/node_modules/.pnpm/testcafe@3.6.2/node_modules:/mnt/c/Users/Usuario/Desktop/UTN/Desarrollo de Software/PoC/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../testcafe/bin/testcafe-with-v8-flag-filter.js" "$@"
else
  exec node  "$basedir/../testcafe/bin/testcafe-with-v8-flag-filter.js" "$@"
fi
